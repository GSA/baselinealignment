  "bugs": {
    "url": "https://github.com/Section508Coordinators/baselinealignment/issues"
  },
  "homepage": "https://github.com/Section508Coordinators/baselinealignment#readme",
  "dependencies": {
    "uswds": "~2.13.2"
  },
  "devDependencies": {
    "a11y": "^0.5.1",
    "rimraf": "^3.0.2"
  }
}
